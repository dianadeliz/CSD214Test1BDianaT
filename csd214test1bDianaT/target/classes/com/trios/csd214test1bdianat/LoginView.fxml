<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" spacing="10" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.trios.csd214test1bdianat.LogInController">
<Label text="Test 1B Login Page" style="-fx-font-size: 18px; -fx-font-weight: bold;"/>
<TextField fx:id="usernameField" promptText="Username"/>
<PasswordField fx:id="passwordField" promptText="Password"/>
<Button text="Login" onAction="#handleLogin"/>
<Label fx:id="messageLabel" textFill="red"/>
</VBox>
